      function dropPin(i){
          if(i < places.length)
            window.setTimeout('dropPin('+(i+1)+')', 1000);
          else
            window.setTimeout('dropHometownPin(0)', 1000);

          geocoder.geocode( { 'address': places[i] }, function(r, s){
              if(s == google.maps.GeocoderStatus.OK){
                  marker = new google.maps.Marker({ map: map, position: r[0].geometry.location });
              }
          });
      }

      function dropHometownPin(i){
          geocoder.geocode( { 'address': hometowns[i] }, function(r, s){
              if(s == google.maps.GeocoderStatus.OK){
                  marker = new google.maps.Marker({ map: map, icon: "http://facebook.andymoreland.com:4567/blue-dot.png", position: r[0].geometry.location });
              } 
          });
          window.setTimeout('dropHometownPin('+(i+1)+')', 1000);
      }

